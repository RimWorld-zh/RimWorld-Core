                </li>
              </points>
            </curve>
          </li>
        </paramMappings>      
        <sustainLoopDurationRange>
          <min>5</min>        
          <max>5</max>
        </sustainLoopDurationRange>      
        <sustainIntervalRange>
          <min>-2.5</min>        
          <max>-2.5</max>
        </sustainIntervalRange>      
        <sustainAttack>2.5</sustainAttack>      
        <sustainRelease>2.5</sustainRelease>
      </li>
    </subSounds>
  </SoundDef>

