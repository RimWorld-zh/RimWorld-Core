<?xml version="1.0" encoding="utf-8"?><Defs>

  <SoundDef>
    <defName>ClockStop</defName>  
    <eventNames />  
    <maxSimultaneous>1</maxSimultaneous>  
    <subSounds>
      <li>
        <onCamera>True</onCamera>      
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>UI/Clock/ClockStops</clipPath>
          </li>
        </grains>      
        <volumeRange>
          <min>15</min>        
          <max>15</max>
        </volumeRange>
      </li>
    </subSounds>
  </SoundDef>

