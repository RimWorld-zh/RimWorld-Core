			</categories>
			<thingDefs>
				<li>Kidney</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>LeftKidney</li>
			<li>RightKidney</li>
		</appliedOnFixedBodyParts>
	</RecipeDef>
	
