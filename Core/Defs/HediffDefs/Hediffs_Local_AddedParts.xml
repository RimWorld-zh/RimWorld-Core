    <label>prosthetic arm</label>
    <labelNoun>a prosthetic arm</labelNoun>
    <comps>
      <li Class="HediffCompProperties_VerbGiver">
        <tools>
          <li>
            <label>fist</label>
            <capacities>
              <li>Blunt</li>
            </capacities>
            <power>9.7</power>
            <cooldownTime>2</cooldownTime>
          </li>
        </tools>
      </li>
    </comps>
    <spawnThingOnRemoved>SimpleProstheticArm</spawnThingOnRemoved>
