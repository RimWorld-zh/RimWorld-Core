<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!-- Added body parts -->

  <HediffDef Name="AddedBodyPartBase" Abstract="True">
    <hediffClass>Hediff_AddedPart</hediffClass>
    <defaultLabelColor>(0.5, 0.5, 0.9)</defaultLabelColor>
    <isBad>false</isBad>
  </HediffDef>

  <HediffDef ParentName="AddedBodyPartBase">
    <defName>PegLeg</defName>
    <label>peg leg</label>
    <labelNoun>a peg leg</labelNoun>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>0.6</partEfficiency>
    </addedPartProps>
    <spawnThingOnRemoved>WoodLog</spawnThingOnRemoved>
  </HediffDef>

  <HediffDef ParentName="AddedBodyPartBase">
    <defName>WoodenFoot</defName>
    <label>wooden foot</label>
    <labelNoun>a wooden foot</labelNoun>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>0.8</partEfficiency>
    </addedPartProps>
    <spawnThingOnRemoved>WoodLog</spawnThingOnRemoved>
  </HediffDef>

  <HediffDef ParentName="AddedBodyPartBase">
