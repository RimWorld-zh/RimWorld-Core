<?xml version="1.0" encoding="utf-8"?>
<DefPackage-TraderKindDef>

  <TraderKindDef>
    <defName>Visitor_Neolithic_Standard</defName>
    <label>trader</label>
    <stockGenerators>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>
          <min>50</min>
          <max>250</max>
        </countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Pemmican</thingDef>
        <countRange>
          <min>40</min>
          <max>80</max>
        </countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>HerbalMedicine</thingDef>
        <countRange>
          <min>1</min>
          <max>4</max>
        </countRange>
      </li>
      <li Class="StockGenerator_WeaponsRanged">
        <maxTechLevelGenerate>Neolithic</maxTechLevelGenerate>
        <countRange>
          <min>0</min>
          <max>1</max>
        </countRange>
    </li>
      <li Class="StockGenerator_Category">
        <maxTechLevelGenerate>Neolithic</maxTechLevelGenerate>
        <categoryDef>WeaponsMelee</categoryDef>
        <countRange>
          <min>0</min>
          <max>1</max>
        </countRange>
      </li>
      <li Class="StockGenerator_Category">
				<categoryDef>Drugs</categoryDef>
				<maxTechLevelGenerate>Neolithic</maxTechLevelGenerate>
				<price>Expensive</price>
        <thingDefCountRange>
          <min>-1</min>
          <max>1</max>
        </thingDefCountRange>
        <totalPriceRange>
          <min>200</min>
          <max>400</max>
        </totalPriceRange>
      </li>
      <li Class="StockGenerator_Clothes">
        <countRange>
          <min>0</min>
          <max>1</max>
        </countRange>
      </li>
			<li Class="StockGenerator_BuyArt" />
      <li Class="StockGenerator_BuyWeirdOrganic" />
      <li Class="StockGenerator_MultiDef">
        <thingDefs>
          <li>Cloth</li>
          <li>Chocolate</li>
        </thingDefs>
      </li>
    </stockGenerators>
  </TraderKindDef>

</DefPackage-TraderKindDef>