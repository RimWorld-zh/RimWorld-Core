      <li Class="StockGenerator_SingleDef">
        <thingDef>Steel</thingDef>
        <countRange>
          <min>200</min>
          <max>300</max>
        </countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>WoodLog</thingDef>
        <countRange>
          <min>500</min>
          <max>600</max>
        </countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Cloth</thingDef>
        <countRange>
          <min>200</min>
          <max>300</max>
