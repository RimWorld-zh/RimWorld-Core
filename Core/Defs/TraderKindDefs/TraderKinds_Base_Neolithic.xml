				<countRange>
					<min>4</min>
					<max>12</max>
				</countRange>
			</li>
      <li Class="StockGenerator_Animals">
        <checkTemperature>true</checkTemperature>
        <maxWildness>0.70</maxWildness>
        <tradeTags>
          <li>StandardAnimal</li>
