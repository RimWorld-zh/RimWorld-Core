<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>SeedGenerator</defName>
    <rulePack>
      <rulesStrings>
        <li>seed->[personnamemale]</li>
        <li>seed->[personnamemale]</li>
        <li>seed->[personnamemale]</li>
        <li>seed->[personnamefemale]</li>
        <li>seed->[personnamefemale]</li>
        <li>seed->[personnamefemale]</li>
        <li>seed->[animal]</li>
        <li>seed->[terrainfeature]</li>
        <li>seed->[outlander]</li>
        <li>seed->[apparel]</li>
        <li>seed->[adjective]</li>
        <li>seed->[concept]</li>
        <li>seed->[tribalword]</li>
        <li>seed->[rareseed]</li>
        <li>seed->[rareseed]</li>
        <li>rareseed->[weapon]</li>
        <li>rareseed->[color]</li>
        <li>rareseed->[vegetable]</li>
        <li>rareseed->[game]</li>
        <li>rareseed->[businessname]</li>
        <li>rareseed->[businesstype]</li>
        <li>rareseed->[community]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>tribalword</keyword>
          <path>NameBanks/TribalWords</path>
        </li>
        <li Class="Rule_File">
          <keyword>businessname</keyword>
          <path>NameBanks/Names_Business</path>
        </li>
        <li Class="Rule_File">
          <keyword>businesstype</keyword>
          <path>NameBanks/Nouns_BusinessTypes</path>
        </li>
        <li Class="Rule_PersonName">
          <gender>Male</gender>
          <keyword>personnamemale</keyword>
        </li>
        <li Class="Rule_PersonName">
          <gender>Female</gender>
          <keyword>personnamefemale</keyword>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>


</Defs>