<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerPersonTribal</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->[tribalword] '[animal_badass]' [tribalword]</li>
        <li>r_name->[Color] [tribalword]</li>
        <li>r_name->[tribalword] [tribalword]</li>
        <li>r_name->[tribalword] [tribalword]</li>
        <li>r_name->[tribalword] [Animal]</li>
        <li>r_name->[Animal] [tribalword]</li>
        <li>r_name->[Color] [Animal]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>tribalword</keyword>
          <path>Words/Foreign/Tribal</path>
        </li>
        <li Class="Rule_File">
          <keyword>animal_badass</keyword>
          <path>Words/Nouns/Animals_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>animal</keyword>
          <path>Words/Nouns/Animals</path>
        </li>
        <li Class="Rule_File">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

</Defs>
