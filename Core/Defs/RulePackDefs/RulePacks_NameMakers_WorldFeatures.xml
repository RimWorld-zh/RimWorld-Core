<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerUtility</defName>
    <include>
      <li>TribalWord</li>
      <li>OutlanderWord</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name->[metaprefix] [standardname]</li>
        <li>r_name(p=0.05)->[storyname] [suffix]</li>

        <li>standardname->[prefix] [suffix]</li>

        <li>metaprefix(p=10)-></li>
        <li>metaprefix->[Direction]</li>
        <li>metaprefix->[derivative]</li>

        <li>derivative->great</li>
        <li>derivative->greater</li>
        <li>derivative->lesser</li>
        <li>derivative->grand</li>
        <li>derivative->new</li>
        <li>derivative->old</li>
        
        <li>prefix(p=0.5)->[Animal]</li>
        <li>prefix(p=0.5)->[Color]</li>
        <li>prefix(p=0.3)->[Color][Animal]</li>
        <li>prefix(p=0.2)->[Color] [Animal]</li>
        <li>prefix(p=0.3)->[AdjectiveNatural][Animal]</li>
        <li>prefix(p=0.2)->[AdjectiveNatural] [Animal]</li>
        <li>prefix(p=0.3)->[Animal][BodyPart]</li>
        <li>prefix(p=0.2)->[Animal] [BodyPart]</li>
        <li>prefix->[Color][NaturalObject]</li>
        <li>prefix->[AdjectiveNatural][BodyPart]</li>
        <li>prefix(p=2)->[tribalword]</li>
        <li>prefix(p=2)->[outlanderword]</li>

        <li>storyname->[subject]'s [storyderivative] folly</li>
        <li>storyname->[subject]'s [storyderivative] vice</li>
        <li>storyname->[subject]'s [storyderivative] lunacy</li>
        <li>storyname->[subject]'s [storyderivative] mistake</li>
        <li>storyname->[subject]'s [storyderivative] fault</li>
        <li>storyname->[subject]'s [storyderivative] misstep</li>
        <li>storyname->[subject]'s [storyderivative] delusion</li>
        <li>storyname->[subject]'s [storyderivative] joy</li>
        <li>storyname(p=0.5)->[subject]sjoy</li>
        <li>storyname->[subject]'s [storyderivative] delight</li>
        <li>storyname->[subject]'s [storyderivative] wonder</li>
        <li>storyname->[subject]'s [storyderivative] pride</li>
        <li>storyname(p=0.5)->[subject]spride</li>
        <li>storyname->[subject]'s [storyderivative] jewel</li>
        <li>storyname->[subject]'s [storyderivative] prize</li>
        <li>storyname->[subject]'s [storyderivative] bounty</li>
        <li>storyname->[subject]'s [storyderivative] honor</li>
        <li>storyname->[subject]'s [storyderivative] jackpot</li>
        <li>storyname->[subject]'s [storyderivative] trophy</li>
        <li>storyname->[subject]'s [storyderivative] conceit</li>
        <li>storyname->[subject]'s [storyderivative] arrogance</li>
        <li>storyname->[subject]'s [storyderivative] stand</li>
        <li>storyname->[subject]'s [storyderivative] loss</li>
        <li>storyname->[subject]'s [storyderivative] debt</li>
        <li>storyname->[subject]'s [storyderivative] collapse</li>
        <li>storyname->[subject]'s [storyderivative] destruction</li>
        <li>storyname->[subject]'s [storyderivative] cataclysm</li>
        <li>storyname->[subject]'s [storyderivative] ruin</li>
        <li>storyname->[subject]'s [storyderivative] ruination</li>
        <li>storyname->[subject]'s [storyderivative] undoing</li>
        <li>storyname->[subject]'s [storyderivative] downfall</li>
        <li>storyname->[subject]'s [storyderivative] end</li>
        <li>storyname(p=0.5)->[subject]send</li>
        <li>storyname->[subject]'s [storyderivative] retirement</li>
        <li>storyname->[subject]'s [storyderivative] achievement</li>
        <li>storyname->[subject]'s [storyderivative] finale</li>
        <li>storyname->[subject]'s [storyderivative] bluff</li>
        <li>storyname->[subject]'s [storyderivative] ruse</li>
        <li>storyname->[subject]'s [storyderivative] deception</li>
        <li>storyname->[subject]'s [storyderivative] facade</li>
        <li>storyname->[subject]'s [storyderivative] fraud</li>
        <li>storyname(p=0.5)->[subject]'s [Gore]</li>
        <li>storyname->[subject]'s [BodyPart]</li>

        <li>storyderivative-></li>
        <li>storyderivative(p=0.1)->[derivative]</li>

        <li>subject(p=5)->[PersonName]</li>
        <li>subject(p=1)->[Animal]</li>
        <li>subject(p=1)->[tribalword]</li>
        <li>subject(p=1)->[outlanderword]</li>
      </rulesStrings>
    </rulePack>
