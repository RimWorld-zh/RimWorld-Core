<?xml version="1.0" encoding="utf-8" ?>
<Defs>

   <!-- RulePacks included in various places but which are not global -->

  <RulePackDef>
    <defName>TribalWordUtility</defName>
    <rulePack>
      <rulesStrings>
        <li>WordTribal(p=3)->[tribal_word_file]</li>
        <li>WordTribal(p=3)->[SylG][SylG]</li>
        <li>WordTribal(p=2)->[SylG][SylG][SylG]</li>
        <li>WordTribal->[SylG][SylG][SylG][SylG]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>tribal_word_file</keyword>
          <path>Words/Foreign/Tribal</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>OutlanderPlaceNameUtility</defName>
    <rulePack>
      <rulesStrings>
        <li>PlaceOutlander->[SylE][place_end]</li>
        <li>PlaceOutlander->[SylE][place_end]</li>
        <li>PlaceOutlander->[SylE][place_end]</li>
        <li>PlaceOutlander->[SylE][SylE][place_end]</li>
        <li>PlaceOutlander->[SylE][SylE][place_end]</li>
        <li>PlaceOutlander->[SylE][SylE][SylE][place_end]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>place_end</keyword>
          <path>WordParts/PlaceEndings</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

</Defs>