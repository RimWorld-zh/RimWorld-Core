<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>ArtName_Weapon</defName>
    <include>
      <li>ArtNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_art_name->[concept_badass]</li>
        <li>r_art_name->The [animal_badass]</li>
        <li>r_art_name->[AdjectiveBadass] [animal_badass]</li>
        <li>r_art_name->[color_badass] [animal_badass]</li>
        <li>r_art_name->[concept_badass] [color_badass]</li>
        <li>r_art_name->[ANYPAWN_nameIndef]'s [concept_badass]</li>
        <li>r_art_name->[concept_badass] of [ANYPAWN_nameIndef]</li>
        <li>r_art_name->[color_badass] [ANYPAWN_nameIndef]</li>
        <li>r_art_name->[ANYPAWN_nameIndef]'s [animal_badass]</li>
        <li>r_art_name->[ANYPAWN_nameIndef]'s [person_badass]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>concept_badass</keyword>
          <path>Words/Nouns/Concepts_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>animal_badass</keyword>
          <path>Words/Nouns/Animals_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>person_badass</keyword>
          <path>Words/Nouns/People_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>color_badass</keyword>
          <path>Words/Nouns/Colors_Badass</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>ArtName_WeaponMelee</defName>
    <include>
      <li>ArtName_Weapon</li>
    </include>
  </RulePackDef>

  <RulePackDef>
    <defName>ArtName_WeaponGun</defName>
    <include>
      <li>ArtName_Weapon</li>
    </include>
  </RulePackDef>


  <RulePackDef>
    <defName>ArtName_Sculpture</defName>
    <include>
      <li>ArtNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_art_name->[ANYPAWN_nameIndef]'s [artname]</li>
        <li>r_art_name->[ANYPAWN_nameIndef] [Trans] [Color]</li>
        <li>r_art_name->[artname] [Trans] [ANYPAWN_nameIndef]</li>
        <li>r_art_name->[artname] [Trans] [Concept]</li>
        <li>r_art_name->[artname] [RomanNumeral]</li>
        <li>r_art_name->[Concept] [Trans] [ANYPAWN_nameIndef]</li>
        <li>r_art_name->[Concept] [Trans] [ANYPAWN_nameIndef]</li>
        <li>r_art_name->[Concept]</li>
        <li>r_art_name->[Color] [Concept]</li>
        <li>r_art_name->[Color] [artname]</li>
        <li>r_art_name->[Color] [TerrainFeature]</li>
        <li>r_art_name->A [art_adjective] [Concept]</li>
        <li>r_art_name->[Concept] No. [Digit]</li>
        <li>r_art_name->[artname] No. [Digit]</li>
        <li>r_art_name->[artname] No. [Digit][Digit]</li>
        <li>r_art_name->[Digit] [Trans] [ANYPAWN_nameIndef]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>


  <RulePackDef>
    <defName>ArtName_Furniture</defName>
    <include>
      <li>ArtNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_art_name->[ANYPAWN_nameIndef]'s [concept_positive]</li>
        <li>r_art_name->[concept_positive]</li>
        <li>r_art_name->The [Animal]</li>
        <li>r_art_name->[color_badass] [Animal]</li>
        <li>r_art_name->[ANYPAWN_nameIndef]'s [Color]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>concept_positive</keyword>
          <path>Words/Nouns/Concepts_Positive</path>
        </li>
        <li Class="Rule_File">
          <keyword>animal</keyword>
          <path>Words/Nouns/Animals</path>
        </li>
        <li Class="Rule_File">
          <keyword>color_badass</keyword>
          <path>Words/Nouns/Colors_Badass</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  
  
  <RulePackDef>
    <defName>ArtName_SarcophagusPlate</defName>
    <include>
      <li>ArtNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_art_name->[AdjectiveAngsty] [ANYPAWN_nameIndef]</li>
        <li>r_art_name->[ANYPAWN_nameIndef]</li>
        <li>r_art_name->[Color] [ANYPAWN_nameIndef]</li>
        <li>r_art_name->[AdjectiveAngsty] [concept_angsty]</li>
        <li>r_art_name->[AdjectiveAngsty] [concept_badass]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>adjective_angsty</keyword>
          <path>Words/Adjectives/Angsty</path>
        </li>
        <li Class="Rule_File">
          <keyword>person_badass</keyword>
          <path>Words/Nouns/People_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
        <li Class="Rule_File">
          <keyword>concept_angsty</keyword>
          <path>Words/Nouns/Concepts_Angsty</path>
        </li>
        <li Class="Rule_File">
          <keyword>concept_badass</keyword>
          <path>Words/Nouns/Concepts_Badass</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>




  <RulePackDef>
    <defName>ArtNameUtility</defName>
    <rulePack>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>art_adjective</keyword>
          <pathList>
            <li>Words/Adjectives/Badass</li>
            <li>Words/Adjectives/Angsty</li>
            <li>Words/Adjectives/Angsty</li>
          </pathList>
        </li>
        <li Class="Rule_File">
          <keyword>artname</keyword>
          <path>Words/Nouns/Artworks</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>



</Defs>
