<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>ArtName_Weapon</defName>
    <include>
      <li>ArtNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>art_name->[badassconcept]</li>
        <li>art_name->The [badassanimal]</li>
        <li>art_name->[badassadjective] [badassanimal]</li>
        <li>art_name->[badasscolor] [badassanimal]</li>
        <li>art_name->[badassconcept] [badasscolor]</li>
        <li>art_name->[anyPawn_nameShort]'s [badassconcept]</li>
        <li>art_name->[badassconcept] of [anyPawn_nameShort]</li>
        <li>art_name->[badasscolor] [anyPawn_nameShort]</li>
        <li>art_name->[anyPawn_nameShort]'s [badassanimal]</li>
        <li>art_name->[anyPawn_nameShort]'s [badassperson]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>badassconcept</keyword>
          <path>Words/Nouns/Concepts_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassanimal</keyword>
          <path>Words/Nouns/Animals_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassperson</keyword>
          <path>Words/Nouns/People_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassconcept</keyword>
          <path>Words/Nouns/Concepts_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>badasscolor</keyword>
          <path>Words/Nouns/Colors_Badass</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>ArtName_WeaponMelee</defName>
    <include>
      <li>ArtName_Weapon</li>
    </include>
  </RulePackDef>

  <RulePackDef>
    <defName>ArtName_WeaponGun</defName>
    <include>
      <li>ArtName_Weapon</li>
    </include>
  </RulePackDef>


  <RulePackDef>
    <defName>ArtName_Sculpture</defName>
    <include>
      <li>ArtNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>art_name->[anyPawn_nameShort]'s [artname]</li>
        <li>art_name->[anyPawn_nameShort] [trans] [color]</li>
        <li>art_name->[artname] [trans] [anyPawn_nameShort]</li>
        <li>art_name->[artname] [trans] [concept]</li>
        <li>art_name->[artname] [romannumeral]</li>
        <li>art_name->[concept] [trans] [anyPawn_nameShort]</li>
        <li>art_name->[concept] [trans] [anyPawn_nameShort]</li>
        <li>art_name->[concept]</li>
        <li>art_name->[color] [concept]</li>
        <li>art_name->[color] [artname]</li>
        <li>art_name->[color] [terrainfeature]</li>
        <li>art_name->A [art_adjective] [concept]</li>
        <li>art_name->[concept] No. [digit]</li>
        <li>art_name->[artname] No. [digit]</li>
        <li>art_name->[artname] No. [digit][digit]</li>
        <li>art_name->[digit] [trans] [anyPawn_nameShort]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>


  <RulePackDef>
    <defName>ArtName_Furniture</defName>
    <include>
      <li>ArtNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>art_name->[anyPawn_nameShort]'s [positiveconcept]</li>
        <li>art_name->[positiveconcept]</li>
        <li>art_name->The [animal]</li>
        <li>art_name->[badasscolor] [animal]</li>
        <li>art_name->[anyPawn_nameShort]'s [color]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>positiveconcept</keyword>
          <path>Words/Nouns/Concepts_Positive</path>
        </li>
        <li Class="Rule_File">
          <keyword>animal</keyword>
          <path>Words/Nouns/Animals</path>
        </li>
        <li Class="Rule_File">
          <keyword>badasscolor</keyword>
          <path>Words/Nouns/Colors_Badass</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  
  
  <RulePackDef>
    <defName>ArtName_SarcophagusPlate</defName>
    <include>
      <li>ArtNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>art_name->[angstyadjective] [anyPawn_nameShort]</li>
        <li>art_name->[anyPawn_nameShort]</li>
        <li>art_name->[color] [anyPawn_nameShort]</li>
        <li>art_name->[angstyadjective] [angstyconcept]</li>
        <li>art_name->[angstyadjective] [badassconcept]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>angstyadjective</keyword>
          <path>Words/Adjectives/Angsty</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassperson</keyword>
          <path>Words/Nouns/People_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
        <li Class="Rule_File">
          <keyword>angstyconcept</keyword>
          <path>Words/Nouns/Concepts_Angsty</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassconcept</keyword>
          <path>Words/Nouns/Concepts_Badass</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>




  <RulePackDef>
    <defName>ArtNameUtility</defName>
    <rulePack>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>art_adjective</keyword>
          <pathList>
            <li>Words/Adjectives/Badass</li>
            <li>Words/Adjectives/Angsty</li>
            <li>Words/Adjectives/Angsty</li>
          </pathList>
        </li>
        <li Class="Rule_File">
          <keyword>artname</keyword>
          <path>Words/Nouns/Artworks</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>



</Defs>
